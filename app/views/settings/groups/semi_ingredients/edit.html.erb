<% content_for(:title) { @semi_ingredient.semi_product.name } %>

<% content_for :breadcrumb do %>
  <%= render "layouts/breadcrumb", {
    nav_links: [
      NavLink.new(path: groups_path, title: t("pages.groups")),
      NavLink.new(path: edit_group_path(@group), title: @group.name),
      NavLink.new(path: edit_group_semi_ingredient_path(@group, @semi_ingredient), title: @semi_ingredient.semi_product.name)
    ]
  } %>
<% end %>

<%= render "form", semi_ingredient: @semi_ingredient %>